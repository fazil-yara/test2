  "scripts": {
    "postinstall": "patch-package && pwft-cli configure --postinstall",
    "configure": "pwft-cli configure",
    "branch": "pwft-cli branch",
    "cli": "pwft-cli",
    "generate": "pwft-generate",
    "commit": "pwft-cli commit",
    "docs:update-toc": "doctoc README.md && prettier --write README.md",
    "start": "yarn types:generate:schema && razzle start",
    "build": "node --huge-max-old-generation-size --max-old-space-size=8192 --max-heap-size=8192 ./node_modules/razzle/scripts/build.js",
    "test": "jest --env=jest-environment-jsdom-sixteen",
    "test:clear-cache": "jest --clearCache",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:start": "node build/server.js",
    "test:e2e:cleanup": "node test/plugins/cleanup.js",
    "test:not:regression": "./node_modules/.bin/cypress-tags run -e TAGS='not @ignore and not @regression'",
    "test:regression": "./node_modules/.bin/cypress-tags run -e TAGS='@regression and not @ignore' --config baseUrl='https://app.staging.at.farm'",
    "test:smoke": "./node_modules/.bin/cypress-tags run -e TAGS='not @ignore and @smoke'",
    "release": "semantic-release",
    "release:notes": "createOrUpdateReleasePR",
    "lint": "eslint \"**/*.tsx\" \"**/*.ts\"",
    "types:check": "tsc --noEmit",
    "types:generate:ops": "graphql-codegen -r dotenv/config -c '.graphqlrc.ops.js'",
    "types:generate:schema": "graphql-codegen -r dotenv/config -c '.graphqlrc.schema.js'",
    "types:generate": "yarn types:generate:schema",
    "types:delete": "find . -name '*.graphql.ts' -delete",
    "translations:merge": "scripts/mergeTranslations.js",
    "translations:locales": "scripts/getSupportedLocales.js",
    "translations:pull": "scripts/expandTranslations.js",
    "translations:compare": "scripts/compareTranslations.js",
    "translations:push": "scripts/pushToPhrase.js",
    "tracking:generate": "typewriter"
  },
